{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/core": "7.12.3",
    "@dnd-kit/core": "^3.0.1",
    "@dnd-kit/modifiers": "^2.0.0",
    "@dnd-kit/sortable": "^3.0.1",
