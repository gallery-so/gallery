    "@ethersproject/providers": "^5.0.24",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@reach/router": "^1.3.4",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
