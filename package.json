{
  "private": true,
  "scripts": {
    "dev": "concurrently -n Next,Relay-Compiler \"next\" \"yarn relay:watch\"",
    "build": "yarn relay && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.tsx,.cjs",
    "typecheck": "tsc --noEmit",
    "relay": "mkdir __generated__; relay-compiler",
    "relay:watch": "mkdir __generated__; relay-compiler --watch",
    "synpress:open": "synpress open",
    "synpress:run": "synpress run  --configFile cypress.json --config supportFile='cypress/support/index.js',pluginsFile='cypress/plugins/index.js'",
