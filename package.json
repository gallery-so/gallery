{
  "private": true,
  "scripts": {
    "dev": "concurrently -n Next,Relay-Compiler,GraphQL-Codegen \"next\" \"yarn relay:watch\" \"graphql-codegen --watch\"",
    "build": "yarn relay && yarn graphql-codegen && yarn upload-apq && next build",
    "upload-apq": "node scripts/upload-persisted-queries.mjs",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.tsx,.cjs",
    "typecheck": "nextjs-routes; graphql-codegen; tsc --noEmit",
    "relay": "mkdir __generated__; touch persisted_queries.json; relay-compiler --repersist",
    "fetch-schema": "curl https://raw.githubusercontent.com/gallery-so/go-gallery/main/graphql/schema/schema.graphql > schema.graphql",
    "relay:watch": "mkdir __generated__; touch persisted_queries.json; relay-compiler --watch --repersist",
    "synpress:open": "synpress open --configFile cypress.config.ts",
    "synpress:run": "synpress run  --configFile cypress.config.ts",
    "cypress:gh": "synpress run --record --key 9d633f22-7630-4a11-9cf4-ff9809332564 --configFile cypress.config.ts",
    "find-unused-modules": "ts-prune -s .graphql.ts -i pages/"
  },
