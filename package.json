{
  "private": true,
  "scripts": {
    "dev": "concurrently -n Next,Relay-Compiler,GraphQL-Codegen \"next\" \"yarn relay:watch\" \"graphql-codegen --watch\"",
    "build": "yarn relay && yarn graphql-codegen && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.tsx,.cjs",
    "typecheck": "graphql-codegen; tsc --noEmit",
    "relay": "mkdir __generated__; relay-compiler",
    "relay:watch": "mkdir __generated__; relay-compiler --watch",
    "synpress:open": "synpress open --configFile cypress.json",
    "synpress:run": "synpress run  --configFile cypress.json --config supportFile='cypress/support/index.js',pluginsFile='cypress/plugins/index.js',fixturesFolder='cypress/fixtures'",
    "cypress:gh": "synpress run --record --key 9d633f22-7630-4a11-9cf4-ff9809332564 --configFile cypress.json --config supportFile='cypress/support/index.js',pluginsFile='cypress/plugins/index.js',fixturesFolder='cypress/fixtures' --group Desktop -b chromium",
    "find-unused-modules": "ts-prune -s .graphql.ts -i pages/"
  },
