{
  "expo": {
    "name": "Gallery Labs",
    "slug": "gallery",
    "privacy": "unlisted",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tbgallery.gallery"
    },
    "android": {
      "blockedPermissions": ["android.permission.RECORD_AUDIO"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tbgallery.gallery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0a46fb2e-423d-42e3-8609-e7772b11d3b2"
      }
    },
    "owner": "tbgallery",
    "plugins": ["sentry-expo"]
  }
}
